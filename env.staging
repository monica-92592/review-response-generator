# Staging Environment Configuration
# Copy this file to .env.staging.local and fill in your actual values

# AI Provider Configuration
OPENAI_API_KEY=sk-your-staging-openai-api-key-here
ANTHROPIC_API_KEY=sk-ant-your-staging-anthropic-api-key-here

# Application Configuration
NEXT_PUBLIC_APP_NAME="AI Review Response Generator (Staging)"
NEXT_PUBLIC_APP_URL=https://staging.your-domain.com

# Security Configuration
ALLOWED_ORIGINS=https://staging.your-domain.com,http://localhost:3000
ENCRYPTION_KEY=your-staging-32-byte-encryption-key-here
MAX_REQUESTS_PER_MINUTE=50
MAX_BULK_REQUESTS_PER_MINUTE=10

# Monitoring and Analytics
MONITORING_ENDPOINT=https://your-monitoring-service.com/api/events
NEXT_PUBLIC_GA_ID=your_staging_google_analytics_id
NEXT_PUBLIC_MIXPANEL_TOKEN=your_staging_mixpanel_token

# Database Configuration (for future features)
DATABASE_URL=postgresql://user:password@staging-db-host:5432/staging_db

# Redis Configuration (for staging rate limiting)
REDIS_URL=redis://staging-redis-host:6379

# External Services
SENTRY_DSN=https://your-sentry-dsn@sentry.io/staging-project-id
LOG_LEVEL=debug

# Performance Configuration
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_MONITORING=true 